{
    "meta": {
        "label": "Create Code Env - DL Plugin",
        "description": "Create Code Env for API DL plugin deployment",
        "icon": "icon-puzzle-piece"
    },
    "impersonate": false,
    "permissions": [],
    "resultType": "HTML",
    "resultLabel": "my production",
    "extension": "txt",
    "mimeType": "text/plain",
    "macroRoles": [],
    "params": [
        {
            "name": "python_interpreter",
            "type": "SELECT",
            "label": "Python interpreter",
            "selectChoices": [
                {
                    "value": "PYTHON35",
                    "label": "Python 3.5 (from PATH)"
                },
                {
                    "value": "PYTHON36",
                    "label": "Python 3.6 (from PATH)"
                },
                {
                    "value": "CUSTOM",
                    "label": "Custom (lookup in PATH)"
                }
            ],
            "defaultValue": "PYTHON36"
        },
        {
            "name": "custom_interpreter",
            "type": "STRING",
            "label": "Python executable",
            "mandatory": true,
            "visibilityCondition": "model.python_interpreter == 'CUSTOM'"
        }
    ]
}
